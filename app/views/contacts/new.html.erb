<h1>Add a Contact!</h1>

<%= form_with model: @contact do |form| %>

  <p>
    <%= form.label "First Name" %>
    <%= form.text_field "first_name" %>
  </p>

  <p>
    <%= form.label "Last Name" %>
    <%= form.text_field "last_name" %>
<p>
    
    <%= form.hidden_field "company_id" %>
  </p>
  <%= form.submit %>
<% end %>